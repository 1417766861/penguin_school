.page-container{
    margin: 0 20rpx;
}
/*抽屉*/
.scroll_box{
    margin: 0;
}
.tag-list{
    padding: 20rpx;
    box-sizing: border-box;
    display: inline-block;
}
.active-text{
    background: linear-gradient(20deg, #6cacff, #8debff);
    border-radius: 10rpx;
    padding: 5rpx 10rpx;
    color: white;
    box-sizing: border-box;
}
.icon-active{
    color: #FFF826;
}
.order-bar{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    font-size: 25rpx;
    padding: 20rpx 10rpx;
    height: 50rpx;
    color: #16D0FF;
}
.count-row{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    width: 100%;
    padding: 0 50rpx;
}
.filter-bar{
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    padding: 10rpx;
    font-size: 25rpx;
}
.search-icon{
    box-sizing: border-box;
    font-size: 35rpx;
    border-radius: 50%;
    color: #16D0FF;
    display: table-cell;
}
.tag-list view{
    margin: 10rpx 5rpx;
    font-size: 25rpx;
    width: 100%;
}
.user-tags{
    margin-left: 30rpx;
    text-align: right;
    margin-top: 5rpx;
    z-index: 99;
}

